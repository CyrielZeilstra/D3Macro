                                    <children>
                                        <HBox prefWidth="366.0">
                                            <children>
                                                <Label text="Key"/>
                                                <ChoiceBox fx:id="button1Selector" prefHeight="25.0" prefWidth="56.0"/>
                                                <Label text="Delay (ms)"/>
                                                <TextField fx:id="button1DelayTxT" prefHeight="25.0" prefWidth="53.0"
                                                           text="1000"/>
                                                <CheckBox fx:id="button1EnabledCheck" mnemonicParsing="false"
                                                          text="Enabled"/>
                                            </children>
                                        </HBox>
                                        <HBox prefWidth="366.0">
                                            <children>
