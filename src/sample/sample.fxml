                                                <Label text="Delay (ms)"/>
                                                <TextField fx:id="button3DelayTxT" prefHeight="25.0" prefWidth="53.0"
                                                           text="1000"/>
                                                <CheckBox fx:id="button3EnabledCheck" mnemonicParsing="false"
                                                          text="Enabled"/>
                                            </children>
                                        </HBox>
                                        <HBox prefWidth="366.0">
                                            <children>
                                                <Label text="Key"/>
