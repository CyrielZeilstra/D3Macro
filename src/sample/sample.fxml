                    </children>
                </VBox>
                <Separator prefWidth="200.0"/>
                <VBox>
                    <children>
                        <HBox alignment="CENTER">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" text="Settings"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="18.0" prefWidth="366.0">
                            <children>
                                <Label text="Start/Stop Key"/>
                                <ChoiceBox fx:id="startstopBtn" prefHeight="25.0" prefWidth="56.0"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </VBox>
